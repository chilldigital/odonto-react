      "parameters": {
        "jsCode": "// Transformar datos de Airtable al formato que espera el frontend\nconst patients = items.map(item => {\n  const fields = item.json.fields;\n  \n  return {\n    id: item.json.id,\n    airtableId: item.json.id,\n    nombre: fields.Nombre || fields.nombre || '',\n    obraSocial: fields['Obra Social'] || fields.obraSocial || '',\n    telefono: fields.Telefono || fields.telefono || '',\n    email: fields.Email || fields.email || '',\n    direccion: fields.Direccion || fields.direccion || '',\n    ultimaVisita: fields['Ultima Visita'] || fields.ultimaVisita || '-',\n    fechaCreacion: fields['Fecha Creacion'] || fields.fechaCreacion || new Date().toISOString(),\n    notas: fields.Notas || fields.notas || ''\n  };\n});\n\nconsole.log(`Transformados ${patients.length} pacientes`);\nreturn [{ json: { patients } }];"
      },
